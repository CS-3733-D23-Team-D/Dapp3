<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="0.0" prefWidth="300.0" spacing="10.0" style="-fx-background-color: #E3E2E6;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
            <children>
                <Label text="Start Node ID">
                    <font>
                        <Font name="Roboto" size="13.0" />
                    </font></Label>
                <TextField style="-fx-background-color: D5E3ff; -fx-border-color: #000000; -fx-border-width: 2;" />
            </children>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
            <children>
                <Label text="End Node ID">
                    <font>
                        <Font name="Roboto" size="13.0" />
                    </font></Label>
                <TextField style="-fx-background-color: D5E3ff; -fx-border-color: #000000; -fx-border-width: 2;" />
            </children>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
            <children>
                <MFXButton style="-fx-background-color: D5E3ff; -fx-border-color: #000000; -fx-border-width: 2;" text="Remove Edge" />
                <MFXButton style="-fx-background-color: D5E3ff; -fx-border-color: #000000; -fx-border-width: 2;" text="Submit" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
